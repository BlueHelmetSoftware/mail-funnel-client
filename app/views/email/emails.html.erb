<%= render 'layouts/navbar' %>

<h3>Email List <%= @list.name %></h3>
<hr>
<h6><%= @list.description %></h6>

<div class="container">
	<ul class="list-group">
		<% @emails.each do |e| %>
				<li class="list-group-item">
					<b><%= e.name %></b> - <%= e.email_address %>
				</li>
		<% end %>
	</ul>

</div>